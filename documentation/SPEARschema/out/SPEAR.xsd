                              <xs:documentation/>
                           </xs:annotation>
                        </xs:enumeration>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="ana">
                  <xs:annotation>
                     <xs:documentation/>
                  </xs:annotation>
