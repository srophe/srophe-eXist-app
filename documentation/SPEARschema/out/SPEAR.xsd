                              <xs:documentation/>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="incerta">
                           <xs:annotation>
                              <xs:documentation/>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="dubia">
                           <xs:annotation>
                              <xs:documentation/>
                           </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="errata">
